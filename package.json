{
  "name": "@reactor/extension-dtm",
  "version": "1.0.0",
  "description": "DTM extension for Turbine",
  "author": {
    "name": "DTM",
    "email": "DTM-Engineering@adobe.com"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:reactor/extension-dtm.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-mcps-release-local"
  },
  "keywords": [
    "turbine",
    "extension",
    "dtm"
  ],
  "scripts": {
    "build": "webpack",
    "test": "reactor-testrunner",
    "lint": "reactor-testrunner lint",
    "sandbox": "(webpack --watch &) && reactor-sandbox",
    "package": "webpack && reactor-packager",
    "bootstrap": "reactor-sandbox init",
    "ci-job": "xvfb-run npm test -- --ci --browser all && npm run lint && npm run package"
  },
  "dependencies": {
    "@coralui/react-coral": "<1.0.0",
    "@reactor/react-components": "^2.3.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "history": "^2.1.2",
    "import-glob-loader": "^1.1.0",
    "nib": "^1.1.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-form": "^5.3.2",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.2.0"
  },
  "devDependencies": {
    "@reactor/extension-support-packager": "^2.2.1",
    "@reactor/extension-support-sandbox": "^7.2.1",
    "@reactor/extension-support-testrunner": "^6.1.1",
    "@reactor/turbine": "^14.2.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "html-webpack-plugin": "^2.22.0",
    "react-addons-test-utils": "^15.3.0",
    "webpack": "^1.13.1"
  }
}
