{
  "name": "@reactor/extension-dtm",
  "version": "2.0.0",
  "description": "DTM extension for Turbine",
  "author": {
    "name": "DTM",
    "email": "DTM-Engineering@adobe.com"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:reactor/extension-dtm.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-mcps-release-local"
  },
  "keywords": [
    "turbine",
    "extension",
    "dtm"
  ],
  "scripts": {
    "build": "webpack --production",
    "test": "reactor-testrunner",
    "lint": "reactor-testrunner lint",
    "sandbox": "webpack --watch --run-sandbox",
    "package": "npm run build && reactor-packager",
    "bootstrap": "reactor-sandbox init",
    "ci-job": "xvfb-run npm test -- --ci --browser firefox chrome ie9sg ie10sg ie11sg edgesg && npm run lint && npm run package"
  },
  "dependencies": {
    "@coralui/react-coral": "<1.0.0",
    "@coralui/redux-form-react-coral": "<1.0.0",
    "@reactor/react-components": "^3.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.0",
    "history": "^4.4.0",
    "import-glob-loader": "^1.1.0",
    "nib": "^1.1.2",
    "postscribe": "^2.0.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-form": "6.2.1",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1"
  },
  "devDependencies": {
    "@reactor/extension-support-packager": "^2.3.3",
    "@reactor/extension-support-sandbox": "^7.8.5",
    "@reactor/extension-support-testrunner": "^9.0.0",
    "@reactor/turbine": "^16.3.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cookie": "^0.3.1",
    "html-webpack-externals-plugin": "2.1.2",
    "html-webpack-plugin": "^2.24.1",
    "react-addons-test-utils": "^15.3.2",
    "webpack": "^1.13.3",
    "webpack-shell-plugin": "^0.4.3",
    "yargs": "^6.3.0"
  }
}
